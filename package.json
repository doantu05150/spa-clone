{
  "name": "spa-website",
  "version": "1.0.0",
  "description": "Nuxt.js project",
  "author": "SangND <dacsang97@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lintfix": "eslint --fix --ext .js,.vue --ignore-path .gitignore ."
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,vue}": [
        "yarn lintfix",
        "git add"
      ],
      "*.{md,json}": [
        "prettier --write",
        "git add"
      ]
    },
    "ignore": [
      "static/**/*.js"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/free-brands-svg-icons": "^5.11.2",
    "@fortawesome/free-solid-svg-icons": "^5.11.2",
    "@fortawesome/vue-fontawesome": "^0.1.7",
    "@nuxtjs/axios": "^5.3.1",
    "@nuxtjs/google-tag-manager": "^2.1.1",
    "axios": "^0.18.0",
    "bootstrap-vue": "2.0.0-rc.19",
    "cookieparser": "^0.1.0",
    "date-fns": "^1.29.0",
    "js-cookie": "^2.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.chunk": "^4.2.0",
    "moment": "^2.24.0",
    "numeral": "^2.0.6",
    "nuxt": "^2.4.3",
    "nuxt-facebook-pixel-module": "^1.1.0",
    "nuxt-fontawesome": "^0.4.0",
    "nuxt-i18n": "^6.3.0",
    "nuxt-start": "^2.4.3",
    "vue-carousel": "^0.16.0-rc2",
    "vuejs-datepicker": "^1.5.4",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-vue": "^5.2.1",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "pm2": "^3.2.9",
    "prettier": "^1.15.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
